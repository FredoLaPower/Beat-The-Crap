[application]

config/name="Fat and Furious"
run/main_scene="res://Scenes/Stage01/Stage01.tscn"
config/icon="res://icon.png"

[autoload]

Music="*res://Singletons/Music/Music.tscn"
Constants="*res://Singletons/Constants.gd"
